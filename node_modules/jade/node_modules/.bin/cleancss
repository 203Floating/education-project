#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/DataSapce/git/education/education-project/node_modules/.store/clean-css@3.4.28/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/DataSapce/git/education/education-project/node_modules/.store/clean-css@3.4.28/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../clean-css@3.4.28/node_modules/clean-css/bin/cleancss" "$@"
else
  exec node  "$basedir/../../../../../clean-css@3.4.28/node_modules/clean-css/bin/cleancss" "$@"
fi
